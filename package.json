{
  "name": "poddy",
  "main": "dist/src/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && cross-env NODE_ENV=development node .",
    "start": "NODE_ENV=production node .",
    "prepare": "husky || prisma generate || true",
    "translate": "tsm languages/utils/interface.ts && biome check --write --no-errors-on-unmatched typings/language.d.ts"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc,prisma}": [
      "biome check --write --no-errors-on-unmatched",
      "prisma format"
    ],
    "languages/{en-US.json,interface.ts}": [
      "pnpm translate",
      "git add typings/language.d.ts"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.896.0",
    "@aws-sdk/lib-storage": "^3.896.0",
    "@discordjs/core": "^2.2.2",
    "@discordjs/rest": "^2.6.0",
    "@discordjs/ws": "^2.0.3",
    "@dsnp/parquetjs": "^1.8.7",
    "@hono/node-server": "^1.19.4",
    "@prisma/client": "^6.4.1",
    "@sapphire/bitfield": "^1.2.4",
    "@sapphire/snowflake": "^3.5.5",
    "@sentry/node": "9.1.0",
    "@types/node-cron": "^3.0.11",
    "bufferutil": "^4.0.9",
    "colorette": "^2.0.20",
    "datadog-metrics": "^0.12.1",
    "discord-api-types": "^0.38.26",
    "dotenv-extended": "^2.9.0",
    "fastify": "^5.2.1",
    "hono": "^4.9.8",
    "i18next": "^25.5.2",
    "i18next-intervalplural-postprocessor": "^3.0.0",
    "node-cron": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@types/got": "^9.6.12",
    "@types/node": "24.5.2",
    "cross-env": "^10.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.0",
    "prisma": "^6.4.1",
    "tsm": "^2.3.0",
    "typescript": "^5.9.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@prisma/client",
      "@prisma/engines",
      "bufferutil",
      "esbuild",
      "prisma"
    ]
  },
  "packageManager": "pnpm@10.17.1"
}
